{
   "theme": "VScode Dark Plus",
   "disable_ai": false,
   "preferred_line_length": 100,
   "soft_wrap": "preferred_line_length",
   "show_wrap_guides": true,
   "wrap_guides": [
      100
   ],
   "max_tabs": 5,
   "base_keymap": "JetBrains",
   "cursor_blink": true,
   "ui_font_weight": 400.0,
   "ui_font_family": ".ZedSans",
   "buffer_font_weight": 400.0,
   "buffer_font_family": "Monaco",
   "tab_size": 3,
   "auto_signature_help": false,
   "show_signature_help_after_edits": true,
   "context_servers": {
      "dart": {
         "command": "dart",
         "args": [
            "mcp-server"
         ]
      }
   },
   "agent": {
      "always_allow_tool_actions": true,
      "inline_assistant_model": {
         "provider": "copilot_chat",
         "model": "gpt-4.1"
      },
      "default_model": {
         "provider": "copilot_chat",
         "model": "gpt-4.1"
      }
   },
   "icon_theme": {
      "mode": "system",
      "light": "Material Icon Theme",
      "dark": "Material Icon Theme"
   },
   "ui_font_size": 20.0,
   "buffer_font_size": 18.0,
   "autosave": {
      "after_delay": {
         "milliseconds": 1000
      }
   },
   "languages": {
      "Dart": {
         "show_edit_predictions": true,
         "code_actions_on_format": {
            "source.organizeImports": true
         }
      },
      "Rust": {
         "show_edit_predictions": true,
         "code_actions_on_format": {
            "source.fixAll.eslint": true,
            "source.removeUnusedImports": true
         }
      },
      "JavaScript": {
         "language_servers": [
            "deno",
            "!typescript-language-server",
            "!vtsls",
            "!eslint"
         ],
         "formatter": "language_server"
      },
      "TypeScript": {
         "language_servers": [
            "deno",
            "!typescript-language-server",
            "!vtsls",
            "!eslint"
         ],
         "formatter": "language_server"
      },
      "TSX": {
         "language_servers": [
            "deno",
            "!typescript-language-server",
            "!vtsls",
            "!eslint"
         ],
         "formatter": "language_server"
      }
   },
   "current_line_highlight": "line",
   "hide_mouse": "on_typing",
   "snippet_sort_order": "bottom",
   "minimap": {
      "current_line_highlight": "line"
   },
   "lsp": {
      "rust-analyzer": {
         "initialization_options": {
            "check": {
               "command": "clippy"
            },
            "cargo": {
               "buildScripts": {
                  "enable": true
               }
            },
            "procMacro": {
               "enable": true
            }
         }
      },
      "dart": {
         "settings": {
            "lineLength": 70
         }
      },
      "deno": {
         "settings": {
            "deno": {
               "enable": true,
               "lint": true,
               "unstable": true,
               "suggest": {
                  "imports": {
                     "hosts": {
                        "https://deno.land": true,
                        "https://esm.sh": true,
                        "npm:": true
                     }
                  }
               }
            }
         }
      }
   },
   "features": {
      "edit_prediction_provider": "copilot"
   },
   "formatter": "language_server",
   "inlay_hints": {
      "enabled": true,
      "show_type_hints": false,
      "show_parameter_hints": true,
      "show_other_hints": false,
      "show_background": false,
      "edit_debounce_ms": 0,
      "scroll_debounce_ms": 50,
      "toggle_on_modifiers_press": null
   },
   "lsp_document_colors": "background",
   "completions": {
      "words": "fallback",
      "words_min_length": 0,
      "lsp": true,
      "lsp_fetch_timeout_ms": 0,
      "lsp_insert_mode": "replace_suffix"
   },
   "whitespace_map": {
      "space": "•",
      "tab": "→"
   },
   "show_completions_on_input": true,
   "tab_bar": {
      "show": true,
      "show_nav_history_buttons": false,
      "show_tab_bar_buttons": false
   },
   "colorize_brackets": true,
   "edit_predictions": {
      "mode": "eager",
      "enabled_in_text_threads": true
   },
   "show_edit_predictions": true,
   "terminal": {
      "env": {}
   },
   "experimental.theme_overrides": {
      "background.appearance": "opaque",
      "editor.background": "#212121B5",
      "panel.background": "#212121B5",
      "hint.background": "#171717B5",
      "warning.background": "#171717B5",
      "syntax": {
         "keyword": {
            "color": "#6388d4",
            "font_style": null,
            "font_weight": null
         },
         "function": {
            "color": "#ffc400",
            "font_style": null,
            "font_weight": null
         },
         "variable": {
            "color": "#d6d6d6",
            "font_style": null,
            "font_weight": null
         },
         "operator": {
            "color": "#b35204",
            "font_style": null,
            "font_weight": null
         },
         "type": {
            "color": "#02b576",
            "font_style": null,
            "font_weight": null
         },
         "comment": {
            "font_style": "italic"
         },
         "comment.doc": {
            "font_style": "italic"
         },
         "attribute": {
            "color": "#02b047",
            "font_style": null,
            "font_weight": null
         },
         "punctuation.markup": {
            "color": "#d07277ff",
            "font_style": null,
            "font_weight": null
         },
         "punctuation.special": {
            "color": "#b1574bff",
            "font_style": null,
            "font_weight": null
         },
         "punctuation.bracket": {
            "color": "#e0bc43",
            "font_style": null,
            "font_weight": null
         },
         "punctuation.delimiter": {
            "color": "#e0bc43",
            "font_style": null,
            "font_weight": null
         },
         "blur_effects": {
            "background_color": "#212121B5",
            "color": "#00000000"
         }
      }
   }
}
